lit: \",\" %}\n        {% for r in ratings %}\n          {% if specs[r] %}\n            {% assign val = specs[r] | default: 4 %}{% assign pos = val | minus: 1 | times: 100 | divided_by: 6 %}\n            <div>\n              <span style=\"font-size: 9px; font-weight: 900; text-transform: uppercase; color: #121212;\">{{ r | replace: '_rating', '' | replace: '_', ' ' }}</span>\n              <div class=\"f-track\"><div class=\"f-dot\" style=\"left: {{ pos }}%;\"></div></div>\n              <div class=\"f-labels\">\n                {% case r %}\n                  {% when 'fit_rating' %}<span>Tight</span><span>Loose</span>\n                  {% when 'softness_rating' %}<span>Rough</span><span>Soft</span>\n                  {% when 'shrinkage_rating' %}<span>Less</span><span>More</span>\n                  {% when 'durability_rating' %}<span>Delicate</span><span>Tough</span>\n                  {% when 'material_quality_rating' %}<span>Basic</span><span>Premium</span>\n                  {% when 'value_rating' %}<span>Fair</span><span>Elite</span>\n                {% endcase %}\n              </div>\n            </div>\n          {% endif %}\n        {% endfor %}\n      </div>\n    </details>\n\n    <details class=\"gear-row\" name=\"gear-accordion\">\n      <summary>Washing <span>+</span></summary>\n      <div style=\"padding-bottom: 2rem;\">\n        <ul style=\"list-style: none; padding: 0; margin: 0; font-size: 14px; color: #424245; line-height: 1.8;\">\n          {% for inst in specs.care_base.value.instructions.value %}<li style=\"display: flex; gap: 12px;\"><span style=\"color: #d2d2d7;\">•</span> {{ inst }}</li>{% endfor %}\n        </ul>\n      </div>\n    </details>\n\n<details class=\"gear-row\" name=\"gear-accordion\">\n  <summary>Measurements <span>+</span></summary>\n  <div style=\"padding-bottom: 30px;\">\n    {% assign chart = specs.size_chart.value %}\n    {% if chart != blank %}\n      <div style=\"overflow-x: auto; margin-top: 10px;\">\n        <table sty